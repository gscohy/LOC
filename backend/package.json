{
  "name": "gestion-locative-backend",
  "version": "2.0.0",
  "description": "Backend API pour l'application de gestion locative",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "npm run db:generate && tsc && tsc-alias",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts",
    "migrate:to-postgres": "node migrate-to-postgresql.js --migrate",
    "migrate:verify": "node migrate-to-postgresql.js --verify",
    "test:postgres": "node test-postgresql.js",
    "setup:database": "node choose-database-provider.js",
    "import:paiements": "node import-paiements-excel.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/pdfkit": "^0.17.2",
    "@types/pg": "^8.15.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "prisma": "^5.7.1",
    "puppeteer": "^21.6.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {}
}
